    "prod": "NODE_ENV=production ts-node src/http.ts",
    "version": "yarn"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "analytics-node": "^3.4.0-beta.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "tslib": "^1.11.0"
  },
  "devDependencies": {
    "@blockstack/stats": "file:../client",
    "@types/analytics-node": "^3.1.2",
    "@types/cors": "^2.8.6",
    "@types/node": "^13.7.4",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
